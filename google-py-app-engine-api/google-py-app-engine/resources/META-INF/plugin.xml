<idea-plugin version="2">
	<id>org.consulo.google-app-engine.py</id>
	<name>Google App Engine Python</name>
	<version>1.0</version>
	<category>Web Frameworks</category>
	<depends>org.consulo.google-app-engine.py.api</depends>

	<extensions defaultExtensionNs="com.intellij">
		<sdkType implementation="org.consulo.google.appengine.py.sdk.PyAppSdkType" />

		<moduleExtensionProvider key="google-app-engine-py"
								 parentKey="python"
								 icon="/icons/appEngine.png"
								 name="Google App Engine"
								 immutableClass="org.consulo.google.appengine.py.module.extension.PyAppModuleExtension"
								 mutableClass="org.consulo.google.appengine.py.module.extension.PyAppMutableModuleExtension" />
	</extensions>
</idea-plugin>