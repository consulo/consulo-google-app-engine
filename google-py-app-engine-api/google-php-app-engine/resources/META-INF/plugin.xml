<idea-plugin version="2">
	<id>org.consulo.google-app-engine.php</id>
	<name>Google App Engine PHP</name>
	<version>1.0</version>
	<category>Custom Languages</category>
	<depends>org.consulo.google-app-engine.py.api</depends>

	<extensions defaultExtensionNs="com.intellij">
		<sdkType implementation="org.consulo.google.appengine.php.sdk.PhpAppSdkType" />

		<moduleExtensionProvider key="google-app-engine-php" parent-key="php"
								 implementationClass="org.consulo.google.appengine.php.module.extension.PhpAppModuleExtensionProvider" />
	</extensions>
</idea-plugin>