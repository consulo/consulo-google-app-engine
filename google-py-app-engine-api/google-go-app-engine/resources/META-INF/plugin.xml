<idea-plugin version="2">
	<id>org.consulo.google-app-engine.go</id>
	<name>Google App Engine Go</name>
	<version>1.0</version>
	<category>Web Frameworks</category>>
	<depends>org.consulo.google-app-engine.py.api</depends>

	<extensions defaultExtensionNs="com.intellij">
		<sdkType implementation="org.consulo.google.appengine.go.sdk.GoAppSdkType" />

		<moduleExtensionProvider key="google-app-engine-go"
								 parentKey="google-go"
								 icon="/icons/appEngine.png"
								 name="Google App Engine"
								 immutableClass="org.consulo.google.appengine.go.module.extension.GoAppModuleExtension"
								 mutableClass="org.consulo.google.appengine.go.module.extension.GoAppMutableModuleExtension" />
	</extensions>
</idea-plugin>