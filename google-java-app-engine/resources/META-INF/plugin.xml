<idea-plugin version="2">
	<id>org.consulo.google-app-engine.java</id>
	<name>Google App Engine Java</name>
	<version>1.0</version>
	<category>Web Frameworks</category>
	<depends>org.consulo.google-app-engine.api</depends>
	<depends>org.mustbe.consulo.java.web</depends>

	<extensions defaultExtensionNs="com.intellij.xml">
		<schemaProvider implementation="org.consulo.google.appengine.java.descriptor.AppEngineWebSchemaProvider"/>
		<dom.fileDescription implementation="org.consulo.google.appengine.java.descriptor.dom.AppEngineWebFileDescription"/>
	</extensions>

	<extensions defaultExtensionNs="com.intellij">
		<sdkType implementation="org.consulo.google.appengine.java.sdk.JavaAppSdkType" />

		<moduleExtensionProvider key="google-app-engine-java" parent-key="java-web"
								 implementationClass="org.consulo.google.appengine.java.module.extension.JavaAppModuleExtensionProvider" />
	</extensions>
</idea-plugin>